{
  "name": "Change My Cursor",
  "description": "Change your cursor to any picture you like!",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "infuse.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Change My Cursor!!",
    "default_icon": "img/cursor.png"
  },
  "manifest_version": 2
}
